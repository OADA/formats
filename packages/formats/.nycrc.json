{
  "reports-dir": ".coverage",
  "temp-dir": ".coverage",
  "extension": [
    ".js",
    ".ts",
    ".schema.json"
  ],
  "include": [
    "src/**/*.ts",
    "https://formats.openag.io/**/*.schema.json"
  ],
  "exclude": [
    "**/*.d.ts",
    "**/*.spec.ts",
    "**/*.schema.ts",
    "src/migrate.ts",
    "src/traverse.ts"
  ],
  "all": true
}
