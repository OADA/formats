    "./trellis/rules/action.js": "./dist/types/trellis/rules/action.js",
    "./trellis/rules/compiled.js": "./dist/types/trellis/rules/compiled.js",
    "./trellis/rules/condition.js": "./dist/types/trellis/rules/condition.js",
