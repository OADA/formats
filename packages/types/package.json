{
  "name": "@oada/types",
  "version": "2.0.0",
  "description": "TypeScript type definitions for OADA formats",
  "keywords": [
    "oada",
    "trellis"
  ],
  "author": "Alex Layton <alex@layton.in>",
  "homepage": "https://github.com/OADA/formats/packages/types",
  "repository": {
    "type": "git",
    "url": "https://github.com/OADA/formats.git",
    "directory": "packages/types"
  },
  "bugs": {
    "url": "https://github.com/OADA/formats/labels/@oada/types"
  },
  "license": "MIT",
  "main": "index.js",
  "directories": {
    "lib": "dist"
  },
  "scripts": {
    "clean": "yarn g:tsc --clean",
    "build": "yarn g:ts-node --files src/compile.ts && yarn g:tsc && cpy '**/*.js' ../ --cwd=src --parents",
    "prestart": "npm run build",
    "prepare": "npm run build",
    "pretest": "npm run build",
    "test": "ava"
  },
  "ava": {
    "files": [
      "**/*.spec.js"
    ]
  },
  "files": [
    "**/*.d.ts",
    "**/*.ts.map",
    "**/*.js",
    "**/*.js.map",
    "tsconfig.json"
  ],
  "dependencies": {
    "ajv": "8.11.0",
    "ajv-formats": "^2.1.1",
    "ajv-formats-draft2019": "^1.6.1"
  },
  "devDependencies": {
    "@apidevtools/json-schema-ref-parser": "^9.0.9",
    "@ava/typescript": "^3.0.1",
    "@oada/formats": "^2.0.0",
    "@types/clone-deep": "^4.0.1",
    "@types/debug": "^4.1.7",
    "@types/json-schema-merge-allof": "^0.6.1",
    "@types/lodash.omit": "^4.5.6",
    "@types/mkdirp": "^1.0.2",
    "@types/node": "^14.18.12",
    "ava": "^3.15.0",
    "clone-deep": "^4.0.1",
    "cpy-cli": "^4.1.0",
    "debug": "^4.3.4",
    "json-schema-merge-allof": "^0.8.1",
    "json-schema-to-typescript": "^10.1.5",
    "jsonschema8": "^1.1.1",
    "lodash.omit": "^4.5.0",
    "mkdirp": "^1.0.4"
  }
}
