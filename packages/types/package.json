  },
  "directories": {
    "lib": "dist"
  },
  "scripts": {
    "clean": "yarn g:tsc --clean && rm -rf types dist",
    "build": "yarn g:tsc && yarn node --enable-source-maps ./dist/src/compile.js && yarn g:tsc",
    "prestart": "npm run build",
    "prepare": "npm run build",
    "pretest": "npm run build",
    "test": "c8 ava",
    "test:debug": "ava -T 60m -svc 1 --no-worker-threads"
  },
  "publishConfig": {
    "access": "public"
  },
  "ava": {
    "failFast": false,
    "files": [
      "**/*.spec.ts"
    ],
    "typescript": {
      "extensions": [
        "ts",
        "cts",
        "mts"
      ],
      "rewritePaths": {
        "src/": "dist/src/",
        "types/": "dist/types/"
      },
      "compile": false
    }
  },
  "c8": {
    "reporter": [
      "text",
      "lcov"
    ],
    "all": true,
    "src": "src",
    "exclude": [
      "src",
      "*.d.ts",
      ".pnp.*",
      ".test"
    ]
  },
  "files": [
    "src",
    "dist",
    "tsconfig.json"
  ],
  "dependencies": {
    "ajv": "8.12.0",
    "ajv-formats": "^2.1.1",
    "ajv-formats-draft2019": "^1.6.1",
    "tslib": "^2.5.0"
  },
  "devDependencies": {
    "@apidevtools/json-schema-ref-parser": "^10.1.0",
    "@ava/typescript": "^4.0.0",
    "@oada/formats": "^3.0.0",
    "@types/clone-deep": "^4.0.1",
    "@types/debug": "^4.1.7",
    "@types/json-schema-merge-allof": "^0.6.1",
    "@types/lodash.omit": "^4.5.7",
    "@types/mkdirp": "^2.0.0",
    "@types/node": "^16.18.31",
    "ava": "5.2.0",
    "c8": "^7.13.0",
    "clone-deep": "^4.0.1",
    "debug": "^4.3.4",
    "json-schema-merge-allof": "^0.8.1",
    "json-schema-to-typescript": "^11.0.5",
    "jsonschema8": "^2.0.0",
    "lodash.omit": "^4.5.0",
    "mkdirp": "^3.0.1"
  }
}